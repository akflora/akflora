    obo:IAO_0000115 "A complexity quality in a bearer by virtue of the bearer's having few parts or being complicated or compound."^^xsd:string ;
    rdfs:label "simple"^^xsd:string ;
    rdfs:subClassOf obo:PATO_0000001 ;
